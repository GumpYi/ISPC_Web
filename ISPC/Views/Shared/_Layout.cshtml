@using StackExchange.Profiling
<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <meta name="description" content="Integrated SPC Tools" />
    <meta name="keywords" content="SPC,SPI,AOI,AXI" />
    <meta name="author" content="GAM_Flextronics" />
    <meta name="robots" content="index,follow" />
    <meta name="application-name" content="www.Flextronics.com" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <!--Site CSS-->
    <link rel="Stylesheet" href="../../Content/themes/bootstrap.min.css" />    
    <link rel="Stylesheet" href="../../Content/themes/bootstrap-switch.min.css" />
    <link rel="Stylesheet" href="../../Content/themes/jquery-ui-1.10.0.custom.css" />
    <link rel="Stylesheet" href="../../Content/themes/font-awesome.min.css" />
    <link rel="Stylesheet" href="../../Content/themes/bootstrap-datetimepicker.css" />
    <link rel="Stylesheet" href="../../Content/themes/ispc/main.css" />
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    @Scripts.Render("~/bundles/modernizr")
    <!--[if IE 7]>
    <link rel="Stylesheet" href="../../Content/themes/font-awesome-ie7.min.css" />        
    <![endif]-->
    <!--[if lt IE 9]>
    <link rel="Stylesheet" href="../../Content/themes/jquery.ui.1.10.0.ie.css" />
    <![endif]-->
    <link rel="apple-touch-icon-precomposed" href="../../Images/Icon/Index.png" />
    <link rel="shortcut icon" href="../../Images/Icon/Index.png"/>
</head>
<body>
    <header>
        <div class="navbar navbar-default navbar-static-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span><span
                            class="icon-bar"></span><span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="../RTIndex/Index">
                        <img src="../../Images/Common/logo.png" alt="logo" height="55" /></a>
                </div>
                <div class="collapse navbar-collapse" role="navigation">
                    <ul class="nav navbar-nav">
                        <li><a href="../RTIndex/Index"><i class="icon-home"></i>&nbsp;Home </a></li>
                        <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">RealTime-Monitor&nbsp;&nbsp;<span
                            class="caret"></span> </a>
                            <ul class="dropdown-menu">
                                <li><a href="../RTSPI/Index">SPI</a> </li>
                                <li><a href="../RTAOI/Index">AOI</a> </li>
                                <li><a href="../RTAXI/Index">AXI</a> </li>
                            </ul>
                        </li>
                        <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Historical-Analysis&nbsp;&nbsp;<span
                            class="caret"></span> </a>
                            <ul class="dropdown-menu">
                                <li><a href="../HASPI/Index">SPI</a> </li>
                                <li><a href="../HAAOI/Index">AOI</a> </li>
                                <li><a href="#">AXI</a> </li>
                            </ul>
                        </li>
                        <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Advance Setting&nbsp;&nbsp;<span
                            class="caret"></span> </a>
                            <ul class="dropdown-menu">
                                <li><a href="../Factory/BuildingConfig">Building</a></li>
                                <li><a href="../Factory/LineList">Line</a></li>
                                <li><a href="../Factory/Machine">Machine</a></li>
                                <li><a href="../Factory/MachineRelated">Machine Related</a></li>
                            </ul>
                        </li>
                        <li><a href="#" class="dropdown-toggle" data-toggle="dropdown">Traceability&nbsp;&nbsp;</a>                           
                        </li>
                       @* <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Setting&nbsp;&nbsp;<span
                            class="caret"></span> </a>
                            <ul class="dropdown-menu">
                                <li><a href="#">User Management</a> </li>
                                <li><a href="#">System Configuration</a> </li>
                                <li><a href="#">Others</a> </li>
                            </ul>
                        </li>*@
                    </ul>
                    <ul class="nav navbar-nav navbar-right hidden-sm hidden-md">
                        <li>                            
                             <div>
                             @Html.Partial("_LoginPartial")  
                             </div>              
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </header>
    <div id="body">
        @RenderSection("featured", required: false)
        <section class="content-wrapper main-content clear-fix">
            @RenderBody()
            @MiniProfiler.RenderIncludes()          
        </section>
    </div>
    <footer>
        <div class="content-wrapper">
            <div class="text-center">
                <p>
                    &copy; @DateTime.Now.Year - ISPC Application . All rights reserved.</p>
            </div>
        </div>
    </footer>
    <script src="../../Scripts/Basic/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/bootstrap.min.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/moment.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/knockout-3.2.0.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/bootstrap-switch.min.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/bootstrap-datetimepicker.min.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/jquery-ui-1.10.0.custom.min.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/highcharts.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/exporting.js" type="text/javascript"></script>    
    <script src="../../Scripts/Basic/spin.js" type="text/javascript"></script>
    <script src="../../Scripts/Basic/jquery.pin.min.js" type="text/javascript"></script>
    <!--[if lt IE 9]>
        <script src="../../Scripts/Basic/html5shiv.js" type="text/javascript"></script>
        <script src="../../Scripts/Basic/respond.js" type="text/javascript"></script>
        <script src="../../Scripts/Basic/excanvas.min.js" type="text/javascript"></script>    
    <![endif]-->
    @RenderSection("scripts", required: false)
</body>
</html>
